<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.connect.mybatis.Authority">

	<resultMap id="authorityResult" type="Authority" >
		<result property="authorityCode" column="authorityCode"></result>
		<result property="authorityName" column="authorityName"></result>
	</resultMap>
	
	<select id="SelectAllAuthority" resultType="Authority">
		SELECT * FROM authority
	</select>
	
</mapper>