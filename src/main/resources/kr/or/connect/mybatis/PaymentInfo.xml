<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="kr.or.connect.paymentInfo">
  	<resultMap type="PaymentInfo" id="paymentInfoResult">
  		<result property="paymentInfoCode" column="PAYMENT_INFO_CODE"/>	
  		<result property="dealCode" column="DEAL_CODE"/>
  		<result property="paymentTf" column="PAYMENT_TF"/>
  		<result property="dealDate" column="DEAL_DATE"/>
  	</resultMap>
  
  	<select id="SelectOne" resultType="PAYMENT_INFO">
  		SELECT paymentInfoCode, dealDate 
  		FROM payment_info 
  		WHERE paymentInfoCode =#{paymentInfoCode}
  	</select>
  
  </mapper>